{
  "manifest_version": 3,
  "name": "reroutr",
  "version": "1.0",
  "description": "Reroute distracting websites to productive ones to stay focused.",
  "permissions": [
    "storage",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "options_page": "options.html",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
